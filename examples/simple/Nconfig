#
# This is to test ncrux-config tools with various options
#

mainmenu "Simple Project Configuration"


config MYBOOL
	bool "My Boolean Option"

config MYSTRING
	string "Message"

config MYTRISTATE
	tristate "My Tristate"
	default m

config MYINT
	int "My Integer"

config MYHEX
	hex "My Hex Value"
